<!--components/song-item-v1/index.wxml-->
<view class="song-item" bindtap="handleSongPlayer" data-id="{{item.id}}">
  <image class="image" src="{{item.al.picUrl}}" mode="widthFix"></image>
  <view class="song-info">
  <view>{{item.name}}</view>
  <view>{{item.ar[0].name}} * {{item.al.name}}</view>
  </view>
  <view>></view>
</view>
